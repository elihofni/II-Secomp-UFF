{
  "name": "escreva-menos-produza-mais",
  "version": "1.0.0",
  "description": "Como organizar projetos web frontend de modo a ser mais produtivo",
  "settings": {
    "pageTitle": "Escreva menos, produza mais"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elihofni/II-Secomp-UFF"
  },
  "scripts": {
    "start": "npm run webpack:dev",
    "webpack:dev": "webpack-dev-server --config webpack.config.dev.js --profile",
    "webpack:prod": "webpack --config webpack.config.prod.js",
    "build": "npm-run-all --serial clean-dist webpack:prod",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint \"./src/scripts/!(vendor)/**/*.js\" \"./src/!(vendor)/*.js\"",
    "lint:scss": "stylelint \"./src/styles/*.scss\" \"./src/styles/**/*.scss\"",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf dist",
    "precommit": "npm run lint",
    "prepush": "npm-run-all --parallel lint test"
  },
  "author": "Elihofni Guirra Lima",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^8.1.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^7.1.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chalk": "^1.1.3",
    "copy-webpack-plugin": "^4.5.0",
    "css-loader": "^0.28.10",
    "dsv-loader": "^2.0.0",
    "eslint": "^3.14.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.1",
    "import-glob-loader": "^1.1.0",
    "imports-loader": "^0.7.0",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.1",
    "postcss-loader": "^2.0.6",
    "pug": "^2.0.0-rc.2",
    "pug-loader": "^2.3.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.13.1",
    "stylelint": "^9.1.1",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-order": "^0.8.1",
    "stylelint-webpack-plugin": "^0.10.3",
    "url-loader": "^1.0.1",
    "webpack": "^3.0.0",
    "webpack-dev-middleware": "^2.0.3",
    "webpack-dev-server": "^2.11.2"
  },
  "dependencies": {
    "babel-plugin-rewire": "^1.0.0",
    "babel-polyfill": "^6.23.0",
    "happypack": "^4.0.1",
    "html-loader": "^0.5.5",
    "pug-html-loader": "^1.1.5",
    "rimraf": "^2.5.4"
  }
}
